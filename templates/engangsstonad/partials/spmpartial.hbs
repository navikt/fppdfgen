        <h3 class="sporsmalstekst">
            {{ this.spørsmålstekst }}
        </h3>
    {{/if}}
    {{#if this.undertekst}}
        <p class="undertekst">
            {{ this.undertekst }}
        </p>
    {{/if }}
    <table>
    {{# each underspørsmål }}
        <tr>
            <td>
                {{spørsmålstekst}}
                {{# each this.underspørsmål as |underspm index| ~}}
                            {{> engangsstonad/partials/spmpartial underspm}}
                {{/each}}
            </td>
            <td>{{svar.0.svar}}</td>
        </tr>
    {{/each}}
    {{# each svar }}
        <tr>
            <td>
                {{ venstretekst }}
                {{#if underspørsmål }}
                <div class="underspm__elementer">
                    {{# each underspørsmål as |underspm index| ~}}
                            {{> engangsstonad/partials/spmpartial underspm}}
                    {{/each}}
                </div>
                {{/if}}
                </td>
            <td>{{ høyretekst }}</td>
        </tr>
    {{/each}}
    </table>
{{/eq}}


        <p class="undertekst">
            {{ this.undertekst }}
        </p>
    {{/if }}
    <table>
    {{# each svar }}
        <tr>
            <td>{{ svardetalj }}</td>
            <td>
                {{# if tom}}
                    {{ iso_to_date fom }} — {{ iso_to_date tom }}
                {{else}}
                    Fra {{ iso_to_date fom }}
                {{/if}}
            </td>
        </tr>
    {{/each}}
    </table>
{{/eq}}

{{#eq svartype "TABELL"}}

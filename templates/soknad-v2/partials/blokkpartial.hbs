{{#eq type "CHECKBOX"}}
    {{#each svar as |s index|~}}
        {{#if checked}}
            <div class="oppsummering__sporsmalscontainer">
                <div class="oppsummering__avkrysset">
                    <img class="checkbox" src="{{ image "Checkboks.png" }}"/>
                    <span class="svar svar--checkbox">{{ spørsmålstekst }}</span>
                </div>
                {{# if undersporsmal }}
                    <div class="oppsummering__undersporsmalsliste">
                        {{# each undersporsmal as |underspm index| ~}}
                            {{> soknad-v2/partials/blokkpartial underspm}}
                        {{/each}}
                    </div>
                {{/if}}
            </div>
        {{/if}}
    {{/each}}
{{/eq}}

{{#eq type "TABELL_OLD"}}
    {{#if this.overskrift}}
        <div class="tabellcontainer">
            <p class="typo-etikett-liten">
                {{ this.overskrift }}
            </p>
    {{/if}}
    {{#if this.undertekst}}
            <p class="undertekst">
                {{ this.undertekst }}
            </p>
    {{/if }}
        <table>
            {{#each tabellrader}}
                <tr>
                    <td>
                        <p>{{ this.venstretekst }}</p>
                    </td>
                    <td><p>{{ this.høyretekst }}</p></td>
                </tr>
                {{#if this.underblokker}}
                    {{#each this.underblokker as |blokk index| ~}}
                        <tr>
                            <td colspan="2">
                                {{> soknad-v2/partials/blokkpartial blokk}}
                            <td>
                        </tr>
                    {{/each}}
                {{/if}}
            {{/each}}
        </table>
    </div>
{{/eq}}

{{#eq type "TABELL"}}
    {{#if this.overskrift}}
        <div class="tabellcontainer">
            <p class="element-overskrift"> <!-- typo-etikett-liten -->
                {{ this.overskrift }}
            </p>
    {{/if}}
    {{#if this.undertekst}}
            <p>{{ this.undertekst }}</p>
    {{/if }}

    {{#each tabellrader}}
            <div class="gruppeelement">
                <div class="leftCol">
                    <p>{{ this.venstretekst }}</p>
                </div>

                <div class="rightCol">
                    <p>{{ this.høyretekst }}</p>
                </div>
            </div>

        {{#if this.underblokker}}
            {{#each this.underblokker as |blokk index| ~}}
                    <div class="gruppeelement">
                        {{> soknad-v2/partials/blokkpartial blokk}}
                    </div>
            {{/each}}
        {{/if}}
    {{/each}}
    </div>
{{/eq}}

{{#eq type "TABELLRAD"}}
    <div class="blokk">
        <div class="leftCol">
            <p>{{ this.venstretekst }}</p>
        </div>

        <div class="rightCol">
            <p>{{ this.høyretekst }}</p>
        </div>
    </div>
{{/eq}}

{{#eq type "LISTE"}}
    <div class="felt">
        <div class="venstre">
            <p>{{ this.tittel }}</p>
        </div>
        <div class="venstre">
            {{#each punkter}}
                <div>
                    <img class="vedleggikon" src="{{ image "vedlegg.png" }}"/>
                    <p class="inline">{{ this }}</p>
                </div>
            {{/each}}
        </div>
    </div>
{{/eq}}

{{#eq type "FELT"}}
    <div class="felt avoid-page-break">
        <p class="element-overskrift">{{ this.felt }}</p>
        <p>{{ this.verdi }}</p>
    </div>
{{/eq}}

{{#eq type "GRUPPE"}}
    <div class="avoid-page-break">
        <p class="element-overskrift">{{ this.overskrift }}</p>
        {{#each tabellrader as |blokk index| ~}}
                {{> soknad-v2/partials/blokkpartial blokk}}
        {{/each}}
    </div>
{{/eq}}

{{#eq type "BULLETPOINT"}}
    <div class="element felt">
        <p class="typo-etikett-liten">{{spørsmålstekst}}</p>
        {{#each svar}}
            <p class="typo-element-feltoppsummering__verdi"> – {{svar}}</p>
        {{/each}}
    </div>
{{/eq}}

{{#eq type "FRITEKST"}}
    <div class="element">
        <p>{{ this.tekst }}</p>
    </div>
{{/eq}}
